{
    "name": "strawberrydev/wimd",
    "description": "Where Is My Data - A Laravel seeder monitoring package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Strawberry Dev"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "laravel/framework": "^9.0|^10.0|^11.0|^12.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Wimd\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wimd\\WimdServiceProvider"
            ],
            "aliases": {
                "Wimd": "Wimd\\Facades\\Wimd"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "keywords": [
        "laravel",
        "seeder",
        "monitoring",
        "data",
        "database"
    ]
}
