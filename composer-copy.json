{
  "name": "your-vendor/wimd",
  "description": "Where Is My Data - A Laravel seeder monitoring package",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Your Name",
      "email": "your.email@example.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/console": "^8.0|^9.0|^10.0",
    "illuminate/support": "^8.0|^9.0|^10.0",
    "illuminate/database": "^8.0|^9.0|^10.0",
    "symfony/console": "^5.0|^6.0"
  },
  "autoload": {
    "psr-4": {
      "Wimd\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Wimd\\WimdServiceProvider"
      ],
      "aliases": {
        "Wimd": "Wimd\\Facades\\Wimd"
      }
    }
  },
  "config": {
    "sort-packages": true
  }
}
